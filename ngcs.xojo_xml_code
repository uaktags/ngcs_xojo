   <SourceLine>Function add_ip(id as string = "", type as string = "IPV4") As String</SourceLine>
   <SourceLine>dim ngcssocket as new ngcs.mysocket</SourceLine>
   <SourceLine>dim opts as new Dictionary</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>select case type</SourceLine>
   <SourceLine>case "ipv4"</SourceLine>
   <SourceLine>type = "IPV4"</SourceLine>
   <SourceLine>case "ipv6"</SourceLine>
   <SourceLine>type = "IPV6"</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>type = "IPV4"</SourceLine>
   <SourceLine>end select</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>opts.value("type") = type</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if id = "" then</SourceLine>
   <SourceLine>if me.id &lt;&gt; "" then</SourceLine>
   <SourceLine>return ngcssocket.Post("servers/"+me.id+"/ips", opts)</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>msgbox "Exception: You have not set a Server ID!"</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>return ngcssocket.Post("servers/"+id + "/ips", opts)</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>End Function</SourceLine>
