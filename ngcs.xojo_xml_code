  <ItemName>get</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function get(id as string = "", page as integer = 1, per_page as integer = 10, sort as string = "name", q as string = "", fields as string = "") As string</SourceLine>
   <SourceLine>dim ngcssocket as new ngcs.mysocket</SourceLine>
   <SourceLine>dim opts as new Dictionary</SourceLine>
   <SourceLine>if id = "" then</SourceLine>
   <SourceLine>opts.Value("page") = page</SourceLine>
   <SourceLine>opts.Value("per_page") = per_page</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if sort &lt;&gt; "" then</SourceLine>
   <SourceLine>opts.Value("sort") = sort</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>if q &lt;&gt; "" then</SourceLine>
   <SourceLine>opts.Value("q") = str(q)</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>if fields &lt;&gt; "" then</SourceLine>
   <SourceLine>opts.Value("fields") = fields</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>if id = "" then</SourceLine>
   <SourceLine>return ngcssocket.Get("servers", opts)</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>return ngcssocket.Get("servers/"+id, opts)</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>End Function</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>0</ItemFlags>
  <IsShared>0</IsShared>
