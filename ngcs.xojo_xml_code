  <ItemName>change_power</ItemName>
  <Compatibility></Compatibility>
  <Visible>1</Visible>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function change_power(id as string = "", action as string = "", method as string = "") As String</SourceLine>
   <SourceLine>dim ngcssocket as new ngcs.mysocket</SourceLine>
   <SourceLine>dim opts as new Dictionary</SourceLine>
   <SourceLine>dim act as string</SourceLine>
   <SourceLine>dim meth as string</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>select case action</SourceLine>
   <SourceLine>case ""</SourceLine>
   <SourceLine>Dim e As New RuntimeException</SourceLine>
   <SourceLine>e.Message = "No Action given"</SourceLine>
   <SourceLine>Raise e</SourceLine>
   <SourceLine>case "reboot"</SourceLine>
   <SourceLine>act = "REBOOT"</SourceLine>
   <SourceLine>case "on"</SourceLine>
   <SourceLine>act = "POWER_ON"</SourceLine>
   <SourceLine>case "off"</SourceLine>
   <SourceLine>act = "POWER_OFF"</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>Dim e As New RuntimeException</SourceLine>
   <SourceLine>e.Message = "Unknown Action Provided"</SourceLine>
   <SourceLine>Raise e</SourceLine>
   <SourceLine>end select</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>select case method</SourceLine>
   <SourceLine>case ""</SourceLine>
   <SourceLine>meth = "HARDWARE"</SourceLine>
   <SourceLine>case "hardware"</SourceLine>
   <SourceLine>meth = "HARDWARE"</SourceLine>
   <SourceLine>case "software"</SourceLine>
   <SourceLine>meth = "SOFTWARE"</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>Dim e As New RuntimeException</SourceLine>
   <SourceLine>e.Message = "Unknown Method Provided"</SourceLine>
   <SourceLine>Raise e</SourceLine>
   <SourceLine>end select</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>opts.value("action") = act</SourceLine>
   <SourceLine>opts.value("method") = meth</SourceLine>
   <SourceLine></SourceLine>
   <SourceLine>if id = "" then</SourceLine>
   <SourceLine>if mid = "" then</SourceLine>
   <SourceLine>Dim e As New RuntimeException</SourceLine>
   <SourceLine>e.Message = "No server ID given"</SourceLine>
   <SourceLine>Raise e</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>return ngcssocket.Get("servers/" +mid + "/status/action", opts)</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>else</SourceLine>
   <SourceLine>return ngcssocket.Get("servers/"+id + "/status/action", opts)</SourceLine>
   <SourceLine>end if</SourceLine>
   <SourceLine>End Function</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
  <AliasName></AliasName>
  <ItemFlags>0</ItemFlags>
  <IsShared>0</IsShared>
  <ItemParams>id as string = "", action as string = "", method as string = ""</ItemParams>
