 <Method>
  <ItemName>Post</ItemName>
  <Compatibility></Compatibility>
  <Attributes></Attributes>
  <Visible>1</Visible>
  <CodeDescription>NGCS Post</CodeDescription>
  <ItemSource>
   <TextEncoding>134217984</TextEncoding>
   <SourceLine>Function Post(url as string, options as Dictionary) As String</SourceLine>
   <SourceLine>url = ngcs.apiurl + url</SourceLine>
   <SourceLine>dim sock as new HTTPSecureSocket</SourceLine>
   <SourceLine>dim opts as Dictionary = options</SourceLine>
   <SourceLine>sock.SetRequestHeader("X-TOKEN", ngcs.apikey)</SourceLine>
   <SourceLine>dim json as new JSONItem</SourceLine>
   <SourceLine>json = opts</SourceLine>
   <SourceLine>sock.SetRequestContent(json.ToString, "application/json")</SourceLine>
   <SourceLine>return sock.Post(url, 15)</SourceLine>
   <SourceLine>End Function</SourceLine>
  </ItemSource>
  <TextEncoding>134217984</TextEncoding>
